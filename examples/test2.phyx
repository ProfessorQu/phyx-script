fn update(self) {
    if (self.frames > 600) {
        if (self.shape == rect) {
            self.shape = circle
        } else {
            self.shape = rect
        }

        self.frames = 0
    } else {
        self.frames = self.frames + 1
    }
}

let c = object {
    update,
    gravity: 3,
    bounciness: 1.5,
    x: 100,
    frames: 0
}

objects.add(c)

